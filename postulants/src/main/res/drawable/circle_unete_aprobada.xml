<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="oval">
            <solid android:color="@color/unete_aprobado" />
            <size
                android:width="@dimen/unete_status_circle"
                android:height="@dimen/unete_status_circle" />
        </shape>
    </item>

    <item
        android:bottom="@dimen/margin_4dp"
        android:left="@dimen/margin_8dp"
        android:right="@dimen/margin_8dp"
        android:top="@dimen/margin_3dp">
        <rotate android:fromDegrees="45">
            <shape android:shape="rectangle">
                <solid android:color="@color/white_3" />
            </shape>
        </rotate>
    </item>

    <item

        android:bottom="@dimen/margin_7dp"
        android:left="@dimen/margin_2dp"

        android:right="@dimen/margin_12dp"
        android:top="@dimen/margin_9dp"

        >
        <rotate android:fromDegrees="45">
            <shape android:shape="rectangle">
                <solid android:color="@color/white_3" />
            </shape>
        </rotate>
    </item>

</layer-list>
