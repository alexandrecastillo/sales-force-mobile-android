<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/txtCampania"
        android:layout_width="wrap_content"
        android:layout_height="17dp"
        android:layout_marginStart="24dp"
        android:fontFamily="@font/mulish_bold"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/view7"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/view7"
        tools:text="C10" />

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/txtGZValue"
        android:layout_width="60dp"
        android:layout_height="17dp"
        android:layout_marginStart="6dp"
        android:layout_marginEnd="6dp"
        android:fontFamily="@font/mulish_regular"
        android:gravity="center_horizontal"
        android:textColor="@color/development_income"
        app:layout_constraintBottom_toBottomOf="@+id/view7"
        app:layout_constraintEnd_toStartOf="@+id/view7"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/txtCampania"
        app:layout_constraintTop_toTopOf="@+id/view7"
        tools:text="180,205" />

    <View
        android:id="@+id/view7"
        android:layout_width="2dp"
        android:layout_height="24dp"
        android:background="@color/baground_space_venta_mn"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/txtSEValue"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/txtGZValue"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txtSEValue"
        android:layout_width="70dp"
        android:layout_height="17dp"
        android:layout_marginStart="4dp"
        android:layout_marginEnd="24dp"
        android:fontFamily="@font/mulish_regular"
        android:gravity="center_horizontal"
        android:textColor="@color/vent_neta"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/view7"
        app:layout_constraintTop_toTopOf="@+id/view7"
        tools:text="157,261" />

</androidx.constraintlayout.widget.ConstraintLayout>
